    crossorigin="anonymous"></script>
<script src="/bts-js/bootstrap.min.js" type="text/javascript"></script>
<script src="/fa-js/all.min.js" type="text/javascript"></script>
<script src="/notif/js/iziToast.js" type="text/javascript"></script>
<script src="https://cdn.jsdelivr.net/npm/flatpickr" type="text/javascript"></script>
<script src="/javascript/mini-event-calendar.min.js" type="text/javascript"></script>

<script>

    var sameDaylastWeek = new Date().setDate(new Date().getDate() - 7);
    var someDaynextMonth = new Date().setDate(new Date().getDate() + 31);

    // All dates should be provided in timestamps
    var sampleEvents = [{ "title": "feed", "date": 1589965629058 }, { "title": "feed", "date": 1589967097232 }, { "title": "feed", "date": 1589967169096 }, { "title": "feed", "date": 1589967179116 }, { "title": "feed", "date": 1589967182812 }, { "title": "feed", "date": 1589967183846 }];

    $(document).ready(function () {
        console.log("ready!");
        $(".flatpickrOne").flatpickr({ enableTime: true })
        $(".flatpickrMult").flatpickr({
            enableTime: true,
            mode: "multiple",
            time_24hr: true
        })

        // FEED NOW !
        $('.feednow').on('click', function () {
            let Status = $(this).val();
            $.ajax({
                type: 'POST',
                url: '/feed',
                success: function (data) {
                    iziToast.success({
                        title: 'Hey ! : ',
                        message: data
                    });

                },
            });
        });

        // Init Calendar
        $("#calendar").MEC({
            from_monday: true,
            events: sampleEvents
        });

    });

</script>